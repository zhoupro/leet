{
  "configurations": {
    "run": {
      "adapter": "delve",
      "filetypes": [ "go" ],
      "variables": {
         "dlvFlags": "--check-go-version=false"
      },
      "configuration": {
        "request": "launch",
        "program": "${fileDirname}",
        "mode": "debug"
      }
    }
  }
}
